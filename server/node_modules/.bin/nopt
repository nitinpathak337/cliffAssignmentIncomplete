#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/sheetscount/server/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/nopt.js/node_modules:/sheetscount/server/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/node_modules:/sheetscount/server/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/node_modules:/sheetscount/server/node_modules/.pnpm/nopt@5.0.0/node_modules:/sheetscount/server/node_modules/.pnpm/node_modules:/sheetscount/server/node_modules:/sheetscount/node_modules:/node_modules:/sheetscount/server/node_modules/nopt/bin/nopt.js/node_modules:/sheetscount/server/node_modules/nopt/bin/node_modules:/sheetscount/server/node_modules/nopt/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
