#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/sheetscount/server/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/bin.js/node_modules:/sheetscount/server/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/sheetscount/server/node_modules/.pnpm/rimraf@3.0.2/node_modules:/sheetscount/server/node_modules/.pnpm/node_modules:/sheetscount/server/node_modules:/sheetscount/node_modules:/node_modules:/sheetscount/server/node_modules/rimraf/bin.js/node_modules:/sheetscount/server/node_modules/rimraf/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
