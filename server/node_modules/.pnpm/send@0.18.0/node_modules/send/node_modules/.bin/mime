#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/sheetscount/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/cli.js/node_modules:/sheetscount/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/sheetscount/server/node_modules/.pnpm/mime@1.6.0/node_modules:/sheetscount/server/node_modules/.pnpm/node_modules:/sheetscount/server/node_modules:/sheetscount/node_modules:/node_modules:/sheetscount/server/node_modules/.pnpm/send@0.18.0/node_modules/send/node_modules:/sheetscount/server/node_modules/.pnpm/send@0.18.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
fi
