#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/sheetscount/server/node_modules/.pnpm/semver@7.0.0/node_modules/semver/bin/semver.js/node_modules:/sheetscount/server/node_modules/.pnpm/semver@7.0.0/node_modules/semver/bin/node_modules:/sheetscount/server/node_modules/.pnpm/semver@7.0.0/node_modules/semver/node_modules:/sheetscount/server/node_modules/.pnpm/semver@7.0.0/node_modules:/sheetscount/server/node_modules/.pnpm/node_modules:/sheetscount/server/node_modules:/sheetscount/node_modules:/node_modules:/sheetscount/server/node_modules/.pnpm/simple-update-notifier@1.1.0/node_modules/simple-update-notifier/node_modules:/sheetscount/server/node_modules/.pnpm/simple-update-notifier@1.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.0.0/node_modules/semver/bin/semver.js" "$@"
else 
  exec node  "$basedir/../../../../../semver@7.0.0/node_modules/semver/bin/semver.js" "$@"
fi
