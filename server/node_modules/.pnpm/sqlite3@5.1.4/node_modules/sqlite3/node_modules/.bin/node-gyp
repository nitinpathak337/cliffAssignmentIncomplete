#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/sheetscount/server/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js/node_modules:/sheetscount/server/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node_modules:/sheetscount/server/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/sheetscount/server/node_modules/.pnpm/node-gyp@8.4.1/node_modules:/sheetscount/server/node_modules/.pnpm/node_modules:/sheetscount/server/node_modules:/sheetscount/node_modules:/node_modules:/sheetscount/server/node_modules/.pnpm/sqlite3@5.1.4/node_modules/sqlite3/node_modules:/sheetscount/server/node_modules/.pnpm/sqlite3@5.1.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
else 
  exec node  "$basedir/../../../../../node-gyp@8.4.1/node_modules/node-gyp/bin/node-gyp.js" "$@"
fi
